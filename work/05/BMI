#include <stdio.h>
int main(void) {
    double Height, Weight, BMI;

    printf("Input your height[cm]: ");
    scanf("%f", &Height);
    printf("Input your weight[kg]: ");
    scanf("%F", &Weight);

    BMI = 10000.0 * Weight / (Height * Height);

    printf("Your BMI is %f.", BMI);
    return 0;
    
}